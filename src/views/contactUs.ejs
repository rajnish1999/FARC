<%- include('partials/header') %> 

    <div class="jumbotron center changingDiv" style="background-image: url('../images/contactus.jpg');">
        <h3 class="display-4 center">Contact us</h3>
    </div>

    <form action="mailto:omgdisha@gmail.com" 
        class="container formClass" 
        method="POST"
        enctype="text/plain"
    >
        <div class="form-group">
            <label for="email"><b>Email address</b></label>
            <input name="Your Email" type="email" class="form-control" id="email" placeholder="Enter email here..">
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
        <div class="form-group">
            <label for="name"><b>Full Name</b></label>
            <input name="Your Full Name" type="text" class="form-control" id="name" placeholder="Enter your full name here..">
        </div>
        <div class="form-group">
            <label for="phone"><b>Phone number</b></label>
            <input name="Your Phone Number" type="tel" id="phone" placeholder="Enter your phone number here.." class="form-control">
        </div>
        <div class="form-group">
            <label for="message"><b>Your Message</b></label>
            <textarea name="Your Message" rows="4" id="message" placeholder="Enter your message here.." class="form-control"></textarea>
        </div>
        <input type="submit" class="btn btn-warning container blockButton" id="sendButton">
    </form>

<%- include('partials/footer') %>